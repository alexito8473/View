<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PrimerAplicacion.Porfolio2Activity">

    <TextView
        android:id="@+id/textPorfolio2Visualizar"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:background="@color/teal_200"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/butPorfolio2Devuelta"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/butPorfolio2Devuelta"
        android:layout_width="150dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Devolver"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textPorfolio2Visualizar" />

</androidx.constraintlayout.widget.ConstraintLayout>